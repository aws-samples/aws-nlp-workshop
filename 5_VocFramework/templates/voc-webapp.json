                "Name" : {"Fn::Sub": "${AWS::StackName}-ApiGatewayDeploymentId" }
            }
        },           
        "WebsiteURL": {
            "Description" : "URL for Unicorn Feedback website",
            "Value": {
                "Fn::Join": ["/", [{"Fn::GetAtt" : [ "WebsiteBucket" , "WebsiteURL" ]}, "unicornfeedback.html"]]
            }
        }         
    }    
}